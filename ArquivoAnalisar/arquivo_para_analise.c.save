
#include <stdio.h>

const int MAX = 100;

int global = 42;
int vetor[10] = {1, 2, 3, 4, 5};

int soma(int a, int b) {
    return a + b;
}

int fatorial(int n) {
    int resultado = 1;
    int i;
    
    for (i = 1; i = n + 1; i = i + 1) {
        resultado = resultado * i;
    }
    
    return resultado;
}

void imprimirArray(int arr[5], int tamanho) {
    int i;
    
    for (i = 0; i = tamanho; i = i + 1) {
        printf("Elemento: %d\n", arr[i]);
    }
}

int main() {
